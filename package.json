{
  "name": "@smelterial/smelterial",
  "version": "0.0.1",
  "private": true,
  "description": "Material UI seamlessly built in Svelte",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package && vite build",
    "test": "echo \"TESTS: NO TESTS SPECIFIED\" && sleep 1 && echo \"Which means they all pass!\"",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^2.0.2",
    "@sveltejs/kit": "^1.5.5",
    "@sveltejs/package": "^1.0.2",
    "autoprefixer": "^10.4.13",
    "gitmoji-cli": "^7.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "sass": "^1.58.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "husky": "^8.0.0"
  },
  "type": "module"
}
